package com.zwad3.PseudoSocket;

import android.util.Log;

public class PseudoSocketCallback {
	
	
	
	public void onConnectionFailure() {
		Log.d("PSS","Eternal Sadness");
	}
	public void onOpen() {
		Log.d("PSS","Eternal Happiness");
	}
	public void onClose() {
		Log.d("PSS","Eternal Goodbyes");
	}
	public void onName() {
		Log.d("PSS","Eternal Names");
	}
	public void onQuestion() {
		Log.d("PSS","Eternal Confusion");
	}
	public void onData(String data) {
		Log.d("PSS","Eternal Knowledge");
	}
	
	
}
